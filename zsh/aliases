# Move around the file system with a few keywords
alias home="cd ~"
alias up="cd ../"
alias back="cd -"
alias edit="code -n"
alias getall="wget -e robots=off -r -nc -np -q"
alias fuck="sudo fc -s"
alias please="sudo fc -s"
alias prune-merged="git checkout master && git fetch -p && git pull && git branch --merged master | grep -v '\* master\|^  master\|^  credit-cards' | xargs -n 1 git branch -d"
alias src="source ~/.zshrc"
alias emulate="open /Applications/Xcode.app/Contents/Developer/Applications/Simulator.app"
alias vim=/usr/local/Cellar/macvim/8.1-153/MacVim.app/Contents/bin/vim
alias tquit="tmux kill-session"

function upstart() {
	if [ ! -z $1 ]; then
		mkdir $1
		cd $1
		git clone --depth=1 --branch=master git@github.com:deanacus/upstart-webpack.git .
		rm -rf .git
	else
		echo "Pass a project folder to create"
	fi
}
